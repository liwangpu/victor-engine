      }
    },
    "dynamic-form": {
      "projectType": "library",
      "root": "projects/dynamic-form",
      "sourceRoot": "projects/dynamic-form/src",
      "prefix": "victor-dynamic",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
